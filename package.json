{
  "name": "chessapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "build": "parcel build index.html",
    "start": "yarn cp && parcel index.html",
    "cp": "cp -R src/assets ./dist/assets || true",
    "dev": "concurrently npm:start npm:proxy",
    "proxy": "local-proxy-cli http://localhost:1234 4321 -H \"Cross-Origin-Embedder-Policy=require-corp\" -H \"Cross-Origin-Opener-Policy=same-origin\" -H \"Content-Security-Policy=img-src 'self' https://chess-board.fly.dev;\""
  },
  "dependencies": {
    "@parcel/transformer-raw": "^2.9.3",
    "algoliasearch": "4",
    "chess.js": "^1.0.0-beta.6",
    "concurrently": "^8.2.1",
    "git": "link:github.com:samuraitruong/local-proxy-cli.git",
    "parcel-packager-wasm": "^0.0.0",
    "postcss": "^8.4.29",
    "react": "18.1.0",
    "react-chessboard": "^4.1.0",
    "react-dom": "18.1.0",
    "react-icons": "^4.10.1",
    "react-instantsearch": "7.0.1",
    "stockfish": "^16.0.0",
    "tailwindcss": "^3.3.3",
    "typesense-instantsearch-adapter": "^2.7.1"
  },
  "devDependencies": {
    "@types/node": "^20.5.8",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "buffer": "^6.0.3",
    "parcel": "2.9.3",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "typescript": "4.7.4"
  }
}
